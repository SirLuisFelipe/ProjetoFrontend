{
  "version": 2,
  "builds": [
    { "src": "api/backend/[...path].js", "use": "@vercel/node@3" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "pages/**/*.html", "use": "@vercel/static" },
    { "src": "Assets/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/backend/(.*)", "dest": "/api/backend/$1" },
    { "src": "/", "dest": "/pages/login.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
